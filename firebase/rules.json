{
  "rules": {
    "rooms": {
      ".read": true
    },
    "questions": {
      "$room": {
        ".read": true,
        ".write": "!data.exists() && auth != null"
      }
    },
    "votes": {
      ".read": true,
      "$questionid": {
        "$userid": {
          ".write": "$userid === auth.uid"
        }
      }
    }
  }
}
